{
  "name": "ngx-filemanager",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "core-build": "ng build --project=ngx-filemanager-core",
    "core-watch": "ng build --project=ngx-filemanager-core --watch",
    "client-build": "ng build --project=ngx-filemanager-client-firebase",
    "client-watch": "ng build --project=ngx-filemanager-client-firebase --watch",
    "client-test": "jest --config projects/ngx-filemanager-client-firebase/jest.config.js --watchAll",
    "client-demo-build": "ng build --project=ngx-filemanager-client-firebase-demo --prod",
    "client-demo-start": "BROWSER=none ng serve --project=ngx-filemanager-client-firebase-demo --watch --vendor-source-map --sourceMap=true --port=4300",
    "client-demo-deploy": "npm run client-demo-build && gh-pages -d dist/ngx-filemanager-client-firebase-demo/",
    "api-firebase-build": "ng build --project=ngx-filemanager-api-firebase",
    "api-firebase-watch": "ng build --project=ngx-filemanager-api-firebase --watch",
    "api-firebase-test": "jest --config projects/ngx-filemanager-api-firebase/jest.config.js --watchAll",
    "api-firebase-demo-debug": "npm run api-firebase-demo-build && node  --trace-warnings ./dist/ngx-filemanager-api-firebase-demo/main.js",
    "api-firebase-demo-build": "ng build --project=ngx-filemanager-api-firebase-demo",
    "api-firebase-demo-watch": "ng build --project=ngx-filemanager-api-firebase-demo --watch",
    "api-firebase-demo-watch-tsc": "tsc --project ./projects/ngx-filemanager-api-firebase-demo/tsconfig.lib.json --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/cdk": "^7.3.6",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/material": "^7.3.6",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "@ctrl/ngx-csv": "^1.1.6",
    "@firebase/storage": "^0.2.12",
    "circular-json": "^0.5.9",
    "core-js": "^2.5.4",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-multipart-file-parser": "^0.1.2",
    "firebase-admin": "^7.2.0",
    "firebase-functions": "^2.2.1",
    "hammerjs": "^2.0.8",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "ngx-auto-table": "^0.3.5",
    "ngx-dropzone-wrapper": "^7.2.1",
    "path-browserify": "^1.0.0",
    "request": "^2.88.0",
    "rxjs": "~6.3.3",
    "uuid": "^3.3.2",
    "verror": "^1.10.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^7.4.3",
    "@angular-builders/jest": "^7.4.1",
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular-devkit/build-ng-packagr": "~0.10.0",
    "@angular-devkit/build-webpack": "^0.13.8",
    "@angular/cli": "~7.0.6",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@linnenschmidt/build-ng-packagr": "^7.0.2",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.11",
    "@types/karma": "^3.0.2",
    "@types/node": "^11.13.5",
    "@types/verror": "^1.10.3",
    "codelyzer": "~4.5.0",
    "jest": "^24.7.1",
    "jest-skipped-reporter": "^0.0.4",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "ts-jest": "^24.0.1",
    "ts-loader": "^5.3.3",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.3",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  }
}
